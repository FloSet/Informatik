/*imperative program*/void main(){
    int a, b, c;
    a=5;
    b=10;
    c = a+b;

    schreib(a + ", " + b + ", " + c);
    b = a;
    a++;
    c = b;
    schreib(a + ", " + b + ", " + c);
    int[] i, k;
    i = new int[2];
    i[0] = 8; i[1] = 9;
    k = i;
    k[1] = 3;
    schreib(i[0] + ", " + i[1]);
    k[0] = 4;
    schreib(" " + i[0]);
    schreib(" " + k[1]);
}